{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "relytestdatafactory"
		},
		"Omni test_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'Omni test'"
		},
		"relyblobtest_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'relyblobtest'"
		},
		"Omni test_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "AKIAIZZHHYHLCPWVGBSQ"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Omni test_secretAccessKey": {
						"value": "[parameters('Omni test_secretAccessKey')]"
					},
					"relyblobtest_connectionString": {
						"value": "[parameters('relyblobtest_connectionString')]"
					},
					"Omni test_properties_typeProperties_accessKeyId": {
						"value": "[parameters('Omni test_properties_typeProperties_accessKeyId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}